{
    "name": "octopyid/debugify",
    "description": "Your Debug Companion",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supian M",
            "email": "supianidz@octopy.dev"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ramsey/uuid": "^4.7",
        "spatie/backtrace": "^1.7",
        "symfony/var-dumper": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Debugify\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\Debugify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
